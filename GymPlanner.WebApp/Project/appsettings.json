{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=sqlserver;Database=gym;User Id=sa;Password=YourPassword123!;Encrypt=True;",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "ConnectionStrings": {
    "SQLExpress": "Server=localhost\\SQLEXPRESS;Database=gym;Trusted_Connection=True;Encrypt=False;",
    "SQLServer": "Server=sqlserver;Database=gym;User Id=sa;Password=YourPassword123!;Encrypt=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DefaultNames": {
    "DEFAULT_EXERCISE_NAME": "Упражнение 1",
    "DEFAULT_FREQUENCY_NAME": "Частота 1",
    "DEFAULT_DESCRIPTION": "0"
  }
}
